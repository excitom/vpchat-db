/* Clear from database all Places To Go records and all
   Persistent Places To Go Records                       */
CREATE PROC clearPTG
AS
BEGIN
  DECLARE @lastError int
  
  BEGIN TRAN
    DELETE PTGlist
    
    SELECT @lastError = @@error
    IF @lastError != 0
    BEGIN
      ROLLBACK TRAN
      RETURN @lastError
    END
    
    DELETE PersistentPTGlist
    
    SELECT @lastError = @@error
    IF @lastError != 0
    BEGIN
      ROLLBACK TRAN
      RETURN @lastError
    END
  COMMIT TRAN
END
GO
